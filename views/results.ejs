<% include partials/header %>

	<title>Results Page</title>
</head>
<body>
	
	
	<h1>Results Page</h1>
	<table>
	  <tr>
	    <th>Year</th>
	    <th>Name</th>
	    <th>Image</th>
	    
	  </tr>
	  <% data['Search'].forEach(function(data){ %>
	  <tr>
	    
	    <td><%= data['Year'] %></td>
	    <td><%= data['Title'] %></td>
	    <td><img src="<%= data['Poster'] %>"></td>
	    
	  </tr>
	  <% }) %>
	</table>
	<br/>

 	
</body>
</html>
